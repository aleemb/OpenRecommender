    <complexType>
      <sequence>
        <element name="choice" type="string" />
        <element name="opponent" type="string" />
        <element name="result" type="string" />
        <element name="points" type="string" />        
        <element name="score" type="string" />        
      </sequence>
    </complexType>
  </element>  
</schema>